{
  "name": "flass",
  "version": "1.0.0",
  "description": "flass",
  "main": "index.js",
  "engines": {
    "node": "6.9.5"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:./client/spec/compiler.js --require babel-polyfill ./client/spec/helper.js ./client/**/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "dev:json": "cross-env BACK_END=\"json\" webpack-dev-server --hot --open --host 0.0.0.0",
    "dev": "cross-env BACK_END=\"rails\" webpack-dev-server --history-api-fallback --hot --open --host 0.0.0.0",
    "dev-rails": "rails s || npm run dev",
    "webpack": "webpack --progress --colors",
    "webpack-watch": "webpack --progress --colors --watch",
    "webpack-production": "webpack --colors --config webpack.config.prod",
    "postinstall": "webpack -p"
  },
  "author": "IP",
  "license": "MIT",
