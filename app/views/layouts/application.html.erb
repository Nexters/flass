<!DOCTYPE html>
<html>
  <head>
    <title>rails-react-boilerplate</title>
    <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= stylesheet_link_tag asset_path('app.css', 'css'), media: 'all' %>
  </head>

  <body>
    <div id="app"></div>
    <%= yield %>
    <%= javascript_include_tag asset_path('app.vendor.js', 'js') %>
    <%= javascript_include_tag asset_path('app.js', 'js') %>
  </body>
</html>
